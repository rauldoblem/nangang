<script src="js/global.js"></script>
<script>
    App = {
        basePath : '{{basePath}}'
    }
</script>
<script src="js/index.js"></script>

<!-- inline scripts related to this page -->
<script type="text/javascript">


    $(document).ready(function() {
        $(document).ajaxSend(function(e, xhr, options) {
            xhr.setRequestHeader('{{tokenHeaderName}}', '{{tokenValue}}');
        });

        App.initSystem();
    });

    //返回按钮
    function backPrevPage(className, title) {
        var vHtml =
                    "  <span class=\"h4 orange\"><i class=\"ace icon fa " + className + "\"></i>                                        "
                    + title
                    + "      <button class=\"btn btn-primary pull-right\" type=\"button\" id=\"backupBtn\">                   "
                    + "          <i class=\"ace-icon fa fa-arrow-left bigger-110\"></i>                                        "
                    + "          返回                                                                                           "
                    + "      </button>                                                                                           "
                    + "  </span>                                                                                                 "
        ;

        $(".page-header").append(vHtml);
        $("#backupBtn").unbind().click(function () {
            window.history.back();
        });
    }
</script>
</body>
</html>