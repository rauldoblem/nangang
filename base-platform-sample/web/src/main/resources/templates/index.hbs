<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta charset="utf-8" />
    <meta name="description" content="overview &amp; stats" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
    <title>sample index</title>

    <!-- bootstrap & fontawesome -->
    <link rel="stylesheet" href="js/ace/css/bootstrap.min.css" />
    <link rel="stylesheet" href="js/components/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="js/components/jquery.gritter/css/jquery.gritter.css"/>
    <!-- page specific plugin styles -->
    <link rel="stylesheet" href="js/components/_mod/jquery-ui/jquery-ui.css"/>
    <link rel="stylesheet"
          href="js/components/bootstrap-datepicker/dist/css/bootstrap-datepicker3.css"/>
    <link rel="stylesheet" href="js/components/_mod/jqgrid/ui.jqgrid.css"/>
    <link rel="stylesheet" href="js/components/ztree/css/zTreeStyle/zTreeStyle.css"/>

    <!-- text fonts -->
    <link rel="stylesheet" href="js/ace/css/ace-fonts.min.css" />

    <!-- ace styles -->
    <link rel="stylesheet" href="js/ace/css/ace.min.css" class="ace-main-stylesheet" id="main-ace-style" />

    <!--[if lte IE 9]>
    <link rel="stylesheet" href="js/ace/css/ace-part2.min.css" class="ace-main-stylesheet" />
    <![endif]-->
    <link rel="stylesheet" href="js/ace/css/ace-skins.min.css" />
    <link rel="stylesheet" href="js/ace/css/ace-rtl.min.css" />

    <!--[if lte IE 9]>
    <link rel="stylesheet" href="js/ace/css/ace-ie.min.css" />
    <![endif]-->
    <link rel="stylesheet" href="css/common.css"/>
    <!-- inline styles related to this page -->

    <!-- ace settings handler -->
    <script src="js/ace/js/ace-extra.min.js"></script>

    <!-- HTML5shiv and Respond.js for IE8 to support HTML5 elements and media queries -->
    <!--[if lte IE 8]>
    <script src="js/components/html5shiv/dist/html5shiv.min.js"></script>
    <script src="js/components/respond/dest/respond.min.js"></script>
    <![endif]-->

</head>

<body class="no-skin">

<div id="navbar" class="navbar navbar-default     wx_topblock     ace-save-state">



</div>

<!-- /section:basics/navbar.layout -->
<div class="main-container ace-save-state" id="main-container">
    <script type="text/javascript">
        try{ace.settings.loadState('main-container')}catch(e){}
    </script>

    <!-- #section:basics/sidebar -->
    <div id="sidebar" class="sidebar responsive ace-save-state">
        <script type="text/javascript">
            try {
                ace.settings.loadState('sidebar')
            } catch (e) {
            }
        </script>

        <ul class="nav nav-list">

            {{#each menus}}
            <li class="
                {{#unless @index}}
                       active
                {{/unless}}
            ">
                <a href="#{{href}}"
                    {{#if childrenMenus}}
                   class="dropdown-toggle"
                    {{/if}}

                    {{#if dataUrl}}
                   data-url="{{dataUrl}}"
                   data-title="{{menuText}}"
                    {{/if}}

                >
                    <i class="menu-icon  {{menuIconClass}}"></i>
                    <span class="menu-text"> {{menuText}} </span>

                    {{#if childrenMenus}}
                        <b class="arrow fa fa-angle-down"></b>
                    {{else}}
                        <b class="arrow"></b>
                    {{/if}}
                </a>

                <b class="arrow"></b>

                {{#if childrenMenus}}
                    <ul class="submenu">
                        {{#each childrenMenus}}
                            <li class="">
                                <a data-url="{{dataUrl}}" data-title="{{menuText}}" href="#{{href}}">
                                    <i class="menu-icon fa fa-caret-right"></i>
                                    {{menuText}}
                                </a>

                                <b class="arrow"></b>
                            </li>
                        {{/each}}
                    </ul>
                {{/if}}
            {{/each}}

        </ul>

    </div>

    <div class="main-content">
        <div class="main-content-inner" id="main-content-inner">

            <div class="breadcrumbs ace-save-state" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li>
                        <i class="ace-icon fa fa-home home-icon"></i>
                        <a href="#">首页</a>
                    </li>
                    <li class="active">工作台</li>
                </ul>




            </div>


            <div class="page-content gridContainer" id="page-content">

            </div>
        </div>
    </div>


    <div class="footer">
        <div class="footer-inner">
            <div class="footer-content">
						<span class="bigger-120">
							北京太极信息系统技术有限公司 &copy; 2017-2018
						</span>
            </div>
        </div>
    </div>
    <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
        <i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
    </a>

</div>

<!-- basic scripts -->

<!--[if !IE]> -->
<script src="js/components/jquery/dist/jquery.js"></script>
<!-- <![endif]-->

